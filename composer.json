{
  "name": "reinfi/zf-guzzle-emitter",
  "type": "library",
  "description": "A Zend Framework module adding subscribers to guzzle emitter via configuration.",
  "keywords": [
    "zend",
    "guzzle",
    "subscriber",
    "emitter"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Reinfi"
    }
  ],
  "require": {
    "php": ">=7.0",
    "zendframework/zend-modulemanager": "~2.7",
    "guzzlehttp/guzzle": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "Reinfi\\GuzzleEmitter\\": [
        "src/",
        "test/"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "zendframework/zend-servicemanager": "~2.7",
    "zendframework/zend-mvc": "~2.7",
    "satooshi/php-coveralls": "^1.0",
    "phpunit/phpcov": "^3.0"
  }
}
